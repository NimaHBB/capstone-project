    "hosting": {
      "port": 5055
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "functions": {
      "port": 5001
    }
  },
  "remoteconfig": {},
  "functions": {
    "source": "functions"
  }
}
